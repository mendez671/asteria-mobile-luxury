{
  "version": 2,
  "name": "asteria-mvp",
  "alias": ["asteria-mvp-test.vercel.app"],
  "build": {
    "env": {
      "NEXT_PUBLIC_APP_NAME": "Asteria",
      "NEXT_PUBLIC_APP_VERSION": "1.0.0-mvp"
    }
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
} 